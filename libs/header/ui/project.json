{
	"name": "header",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/header/ui/src",
	"prefix": "angular-monorepo",
	"tags": [],
	"projectType": "library",
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/header/ui/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/header/ui/**/*.ts",
					"libs/header/ui/**/*.html"
				]
			}
		},
		"storybook": {
			"executor": "@storybook/angular:start-storybook",
			"options": {
				"port": 4400,
				"configDir": "libs/header/ui/.storybook",
				"browserTarget": "header:build-storybook",
				"compodoc": false
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"build-storybook": {
			"executor": "@storybook/angular:build-storybook",
			"outputs": ["{options.outputDir}"],
			"options": {
				"outputDir": "dist/storybook/header",
				"configDir": "libs/header/ui/.storybook",
				"browserTarget": "header:build-storybook",
				"compodoc": false
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"test-storybook": {
			"executor": "nx:run-commands",
			"options": {
				"command": "test-storybook -c libs/header/ui/.storybook --url=http://localhost:4400"
			}
		},
		"static-storybook": {
			"executor": "@nx/web:file-server",
			"options": {
				"buildTarget": "header:build-storybook",
				"staticFilePath": "dist/storybook/header"
			},
			"configurations": {
				"ci": {
					"buildTarget": "header:build-storybook:ci"
				}
			}
		}
	}
}
